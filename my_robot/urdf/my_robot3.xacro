<?xml version='1.0'?>
<robot name="my_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:include filename="$(find my_robot)/urdf/my_robot.gazebo" />
  <link name="robot_footprint">
  </link>
  
  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/>
	<parent link="robot_footprint"/>
	<child link="chassis"/>
  </joint>
  
  <link name="chassis">
	<inertial>
	  <mass value="20"/>
          <origin xyz="0.0 0 0.1" rpy=" 0 0 0"/>
           <inertia
             ixx="0.1" ixy="0" ixz="0"
             iyy="0.1" iyz="0"
             izz="0.1"
          />
        </inertial>
	<visual>
	  <origin xyz="0 0 0.1" rpy="0 0 0"/>
	  <geometry>
	    <cylinder length="0.4" radius="0.15"/>
	  </geometry>
	</visual>
	<collision>
	  <origin xyz="0 0 0.1" rpy="0 0 0"/>
	  <geometry>
	    <cylinder length="0.4" radius="0.15"/>
	  </geometry>
	</collision>
  </link>	

  <link name="head">
	<visual>
	  <origin xyz="0 0 0" rpy="0 0 0"/>
	  <geometry>
	    <sphere radius="0.15"/>
	  </geometry>
	</visual>
	<collision>
	  <origin xyz="0 0 0" rpy="0 0 0"/>
	  <geometry>
	    <sphere radius="0.15"/>
	  </geometry>
	</collision>
  </link>

  <link name="left_leg">
	<inertial>
	  <mass value="5"/>
          <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
           <inertia
             ixx="0.1" ixy="0" ixz="0"
             iyy="0.1" iyz="0"
             izz="0.1"
          />
        </inertial>
	<visual>
	  <origin xyz="0 0 0" rpy="0 0 0"/>
	  <geometry>
	    <box size="0.2 0.1 0.4"/>
	  </geometry>
	</visual>
	<collision>
	  <origin xyz="0 0 0" rpy="0 0 0"/>
	  <geometry>
	    <box size="0.2 0.1 0.4"/>
	  </geometry>
	</collision>
  </link>	
  <link name="left_leg_base">
	<inertial>
	  <mass value="5"/>
          <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
           <inertia
             ixx="0.1" ixy="0" ixz="0"
             iyy="0.1" iyz="0"
             izz="0.1"
          />
        </inertial>
	<visual name="left_leg_base_visual">
	  <origin xyz="0 0 0" rpy="0 0 0"/>
	  <geometry>
	    <box size="0.4 0.1 0.1"/>
	  </geometry>
	</visual>
	<collision name="left_leg_base_collision">
	  <origin xyz="0 0 0" rpy="0 0 0"/>
	  <geometry>
	    <box size="0.4 0.1 0.1"/>
	  </geometry>
	</collision>
  </link>

  <link name="right_leg_base">
	<inertial>
	  <mass value="5"/>
          <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
           <inertia
             ixx="0.1" ixy="0" ixz="0"
             iyy="0.1" iyz="0"
             izz="0.1"
          />
        </inertial>
	<visual name="right_leg_base_visual">
	  <origin xyz="0 0 0" rpy="0 0 0"/>
	  <geometry>
	    <box size="0.4 0.1 0.1"/>
	  </geometry>
	</visual>
	<collision name="right_leg_base_collision">
	  <origin xyz="0 0 0" rpy="0 0 0"/>
	  <geometry>
	    <box size="0.4 0.1 0.1"/>
	  </geometry>
	</collision>
  </link>	

  <link name="right_leg">
	<inertial>
	  <mass value="5"/>
          <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
           <inertia
             ixx="0.1" ixy="0" ixz="0"
             iyy="0.1" iyz="0"
             izz="0.1"
          />
        </inertial>
	<visual>
	  <origin xyz="0 0 0" rpy="0 0 0"/>
	  <geometry>
	    <box size="0.2 0.1 0.4"/>
	  </geometry>
	</visual>
	<collision>
	  <origin xyz="0 0 0" rpy="0 0 0"/>
	  <geometry>
	    <box size="0.2 0.1 0.4"/>
	  </geometry>
	</collision>
  </link>


  <link name="left_wheel">
	<inertial>
	  <mass value="2"/>
          <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
          <inertia
             ixx="0.1" ixy="0" ixz="0"
             iyy="0.1" iyz="0"
             izz="0.1"
          />
        </inertial>
	<visual>
	  <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
	  <geometry>
	    <cylinder length="0.075" radius="0.1"/>
	  </geometry>
	</visual>
	<collision>
	  <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
	  <geometry>
	    <cylinder length="0.075" radius="0.1"/>
	  </geometry>
	</collision>
  </link>

  <link name="right_wheel">
	<inertial>
	  <mass value="2"/>
          <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
          <inertia
             ixx="0.1" ixy="0" ixz="0"
             iyy="0.1" iyz="0"
             izz="0.1"
          />
        </inertial>
	<visual>
	  <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
	  <geometry>
	    <cylinder length="0.075" radius="0.1"/>
	  </geometry>
	</visual>
	<collision>
	  <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
	  <geometry>
	    <cylinder length="0.075" radius="0.1"/>
	  </geometry>
	</collision>
  </link>

  <link name="left_wheel_back">
	<inertial>
	  <mass value="3"/>
          <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
          <inertia
             ixx="0.1" ixy="0" ixz="0"
             iyy="0.1" iyz="0"
             izz="0.1"
          />
        </inertial>
	<visual>
	  <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
	  <geometry>
	    <cylinder length="0.075" radius="0.1"/>
	  </geometry>
	</visual>
	<collision>
	  <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
	  <geometry>
	    <cylinder length="0.075" radius="0.1"/>
	  </geometry>
	</collision>
  </link>

  <link name="right_wheel_back">
	<inertial>
	  <mass value="3"/>
          <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
          <inertia
             ixx="0.1" ixy="0" ixz="0"
             iyy="0.1" iyz="0"
             izz="0.1"
          />
        </inertial>
	<visual>
	  <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
	  <geometry>
	    <cylinder length="0.075" radius="0.1"/>
	  </geometry>
	</visual>
	<collision>
	  <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
	  <geometry>
	    <cylinder length="0.075" radius="0.1"/>
	  </geometry>
	</collision>
  </link>
  <link name="camera">
	<inertial>
	  <mass value="0"/>
          <origin xyz="0.0 0 0.0" rpy=" 0 0 0"/>
           <inertia
             ixx="0.1e-6" ixy="0" ixz="0"
             iyy="0.1e-6" iyz="0"
             izz="0.1e-6"
          />
        </inertial>
	<visual>
	  <origin xyz="0 0 0" rpy="0 0 0"/>
	  <geometry>
	    <box size="0.05 0.05 0.05"/>
	  </geometry>
	</visual>
	<collision>
	  <origin xyz="0 0 0" rpy="0 0 0"/>
	  <geometry>
	    <box size="0.05 0.05 0.05"/>
	  </geometry>
	</collision>
  </link>

  <link name="hokuyo">
	<inertial>
	  <mass value="1e-5"/>
          <origin xyz="0.0 0 0.0" rpy=" 0 0 0"/>
           <inertia
             ixx="0.1e-6" ixy="0" ixz="0"
             iyy="0.1e-6" iyz="0"
             izz="0.1e-6"
          />
        </inertial>
	<visual>
	  <origin xyz="0 0 0" rpy="0 0 0"/>
	  <geometry>
            <mesh filename="package://my_robot/meshes/hokuyo.dae"/>
	  </geometry>
	</visual>
	<collision>
	  <origin xyz="0 0 0" rpy="0 0 0"/>
	  <geometry>
	    <box size="0.1 0.1 0.1"/>
	  </geometry>
	</collision>
  </link>

  <link name="box">
	<visual>
	  <origin xyz="0 0 0" rpy="0 0 0"/>
	  <geometry>
	    <box size="0.05 0.05 0.05"/>
	  </geometry>
	</visual>
	<collision>
	  <origin xyz="0 0 0" rpy="0 0 0"/>
	  <geometry>
	    <box size="0.05 0.05 0.05"/>
	  </geometry>
	</collision>
  </link>

  <joint type="fixed" name="left_leg_joint">
    <origin xyz = "0 0.15 0" rpy="0 0 0"/>
    <child link = "left_leg"/>
    <parent link = "chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>

  <joint type="fixed" name="left_leg_base_joint">
    <origin xyz = "0 0 -0.25" rpy="0 0 0"/>
    <child link = "left_leg_base"/>
    <parent link = "left_leg"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>

  <joint type="fixed" name="right_leg_joint">
    <origin xyz = "0 -0.15 0" rpy="0 0 0"/>
    <child link = "right_leg"/>
    <parent link = "chassis"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
  </joint>

  <joint type="fixed" name="right_leg_base_joint">
    <origin xyz = "0 0 -0.25" rpy="0 0 0"/>
    <child link = "right_leg_base"/>
    <parent link = "right_leg"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
  </joint>

  <joint type="fixed" name="head_joint">
    <origin xyz = "0 0 0.3" rpy="0 0 0"/>
    <child link = "head"/>
    <parent link = "chassis"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
  </joint>

  <joint type="continuous" name="left_wheel_hinge">
    <origin xyz = "0.2 0.0875 0" rpy="0 0 0"/>
    <child link = "left_wheel"/>
    <parent link = "left_leg_base"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="0.5" friction="10.0"/>
  </joint>

  <joint type="continuous" name="right_wheel_hinge">
    <origin xyz = "0.2 -0.0875 0" rpy="0 0 0"/>
    <child link = "right_wheel"/>
    <parent link = "right_leg_base"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="0.5" friction="10.0"/>
  </joint>

  <joint type="continuous" name="left_wheel_back_hinge">
    <origin xyz = "-0.2 0.0875 0" rpy="0 0 0"/>
    <child link = "left_wheel_back"/>
    <parent link = "left_leg_base"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="0.5" friction="10.0"/>
  </joint>

  <joint type="continuous" name="right_wheel_back_hinge">
    <origin xyz = "-0.2 -0.0875 0" rpy="0 0 0"/>
    <child link = "right_wheel_back"/>
    <parent link = "right_leg_base"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="0.5" friction="10.0"/>
  </joint>

  <joint type="fixed" name="camera_joint">
    <origin xyz = "0.15 0 0" rpy="0 0 0"/>
    <child link = "camera"/>
    <parent link = "chassis"/>
    <axis xyz="1 0 0" rpy="0 0 0"/>
  </joint>

  <joint type="fixed" name="box_joint">
    <origin xyz = "0.12 0 0.1" rpy="0 0 0"/>
    <child link = "box"/>
    <parent link = "head"/>
    <axis xyz="1 0 0" rpy="0 0 0"/>
  </joint>

  <joint type="fixed" name="hokuyo_joint">
    <origin xyz = "0 0 0.14" rpy="0 0 0"/>
    <child link = "hokuyo"/>
    <parent link = "head"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
  </joint>

  <gazebo reference="chassis">
   <material>Gazebo/Blue</material>
  </gazebo>

  <gazebo reference="box">
   <material>Gazebo/Blue</material>
  </gazebo>

  <gazebo reference="camera">
   <material>Gazebo/Black</material>
  </gazebo>

</robot>
